{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Uiua",
	"patterns": [
		{
			"include": "#comments"
		},
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#characters"
		},
		{
			"include": "#numbers"
		},
		{
			"include": "#strand"
		},
		{
			"include": "#noadic"
		},
		{
			"include": "#monadic"
		},
		{
			"include": "#dyadic"
		},
		{
			"include": "#mod1"
		},
		{
			"include": "#mod2"
		}
	],
	"repository": {
		"comments": {
			"name": "comment.line.uiua",
			"match": "#.*$"
		},
		"strings": {
			"name": "constant.character.escape",
			"match": "(\".*\"|\\$.*$)"
		},
		"characters": {
			"name": "constant.character.escape",
			"match": "'\\\\?.'"
		},
		"numbers": {
			"name": "constant.numeric.uiua",
			"match": "\\d+(\\.\\d+(e[+-]?\\d+)?)?"
		},
		"strand": {
			"name": "comment.line",
			"match": "_"
		},
		"noadic": {
			"name": "entity.name.tag.uiua",
			"match": "([·↬⚂ηπτ∞]|noo(p)?|rec(u(r)?)?|rand(o(m)?)?|eta|pi|tau|inf(i(n(i(t(y)?)?)?)?)?)"
		},
		"monadic": {
			"name": "string.quoted",
			"match": "([.;¬±¯⌵√○⌊⌈⁅⧻∴△⇡⊢⌀⇌♭⋯⍉⌂⊛⊝↲|!⎋]|not|sig(n)?|abs(o(l(u(t(e( (v(a(l(u(e)?)?)?)?)?)?)?)?)?)?)?|sqr(t)?|sin(e)?|flo(o(r)?)?|cei(l(i(n(g)?)?)?)?|rou(n(d)?)?|len(g(t(h)?)?)?|rank|sha(p(e)?)?|rang(e)?|fir(s(t)?)?|tru(n(c(a(t(e)?)?)?)?)?|rev(e(r(s(e)?)?)?)?|des(h(a(p(e)?)?)?)?|bit(s)?|tra(n(s(p(o(s(e)?)?)?)?)?)?|gra(d(e)?)?|cla(s(s(i(f(y)?)?)?)?)?|ded(u(p(l(i(c(a(t(e)?)?)?)?)?)?)?)?|wai(t)?|bre(a(k)?)?|pars(e)?|gen)"
		},
		"dyadic": {
			"name": "entity.name.function.uiua",
			"match": "([,∶=≠<≤>≥+\\-×÷◿ⁿₙ↧↥∠≅⊂⊟⍛⊡⊏↙↘↯↻◫‡∊⌕⊗⊕⊜⍤]|mod(u(l(u(s)?)?)?)?|pow(e(r)?)?|log(a(r(i(t(h(m)?)?)?)?)?)?|min(i(m(u(m)?)?)?)?|max(i(m(u(m)?)?)?)?|ata(n(g(e(n(t)?)?)?)?)?|mat(c(h)?)?|joi(n)?|cou(p(l(e)?)?)?|fil(l)?|pi(c(k)?)?|sel(e(c(t)?)?)?|tak(e)?|dro(p)?|res(h(a(p(e)?)?)?)?|rot(a(t(e)?)?)?|win(d(o(w(s)?)?)?)?|repl(i(c(a(t(e)?)?)?)?)?|mem(b(e(r)?)?)?|fin(d)?|ind(e(x(o(f)?)?)?)?|gro(u(p)?)?|part(i(t(i(o(n)?)?)?)?)?|ass(e(r(t)?)?)?|use)"
		},
		"mod1": {
			"name": "entity.name.type.uiua",
			"match": "([/∧\\\\∵≡≐⫫⊞⊠⍥↶]|fol(d)?|eac(h)?|row(s)?|dis(t(r(i(b(u(t(e)?)?)?)?)?)?)?|plo(w)?|tab(l(e)?)?|cro(s(s)?)?|repe(a(t)?)?|inv(e(r(t)?)?)?)"
		},
		"mod2": {
			"name": "keyword.control.uiua",
			"match": "([⍜⍚⍣↰]|und(e(r)?)?|lev(e(l)?)?|try|spa(w(n)?)?)"
		}
	},
	"scopeName": "source.uiua"
}