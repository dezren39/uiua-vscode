{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Uiua",
	"patterns": [
		{
			"include": "#comments"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#characters"
		},
		{
			"include": "#numbers"
		},
		{
			"include": "#strand"
		},
		{
			"include": "#noadic"
		},
		{
			"include": "#monadic"
		},
		{
			"include": "#dyadic"
		},
		{
			"include": "#mod1"
		},
		{
			"include": "#mod2"
		}
	],
	"repository": {
		"comments": {
			"name": "comment.line.uiua",
			"match": "#.*$"
		},
		"strings": {
			"name": "constant.character.escape",
			"match": "(\".*\"|\\$.*$)"
		},
		"characters": {
			"name": "constant.character.escape",
			"match": "'\\\\?.'"
		},
		"numbers": {
			"name": "constant.numeric.uiua",
			"match": "\\d+(\\.\\d+(e[+-]?\\d+)?)?"
		},
		"strand": {
			"name": "comment.line",
			"match": "_"
		},
		"noadic": {
			"name": "entity.name.tag.uiua",
			"match": "([↬⚂ηπτ∞]|rec(u(r)?)?|rand(o(m)?)?|eta|pi|tau|inf(i(n(i(t(y)?)?)?)?)?|(scanline|args|now))"
		},
		"monadic": {
			"name": "string.quoted",
			"match": "([¬±¯⌵√○⌊⌈⁅⧻∴△⇡⊢⇌♭⋯⍉⌂⊛⊝□↲!⎋]|not|sig(n)?|abs(o(l(u(t(e( (v(a(l(u(e)?)?)?)?)?)?)?)?)?)?)?|sqr(t)?|sin(e)?|flo(o(r)?)?|cei(l(i(n(g)?)?)?)?|rou(n(d)?)?|len(g(t(h)?)?)?|rank|sha(p(e)?)?|rang(e)?|fir(s(t)?)?|rev(e(r(s(e)?)?)?)?|des(h(a(p(e)?)?)?)?|bit(s)?|tra(n(s(p(o(s(e)?)?)?)?)?)?|gra(d(e)?)?|cla(s(s(i(f(y)?)?)?)?)?|ded(u(p(l(i(c(a(t(e)?)?)?)?)?)?)?)?|con(s(t(a(n(t)?)?)?)?)?|wai(t)?|bre(a(k)?)?|(freadallbytes|freadallstr|tcpconnect|tcpaccept|tcplisten|audioplay|flistdir|fisfile|fexists|fcreate|imshow|imread|import|close|sleep|fopen|print|parse|prin|show|var|gen))"
		},
		"dyadic": {
			"name": "entity.name.function.uiua",
			"match": "([=≠<≤>≥+\\-×÷◿ⁿₙ↧↥∠⊂⊟≅⊡⊏↙↘↯↻◫‡⌕∊⊗⍤]|mod(u(l(u(s)?)?)?)?|pow(e(r)?)?|log(a(r(i(t(h(m)?)?)?)?)?)?|min(i(m(u(m)?)?)?)?|max(i(m(u(m)?)?)?)?|ata(n(g(e(n(t)?)?)?)?)?|joi(n)?|cou(p(l(e)?)?)?|mat(c(h)?)?|pi(c(k)?)?|sel(e(c(t)?)?)?|tak(e)?|dro(p)?|res(h(a(p(e)?)?)?)?|rot(a(t(e)?)?)?|win(d(o(w(s)?)?)?)?|repl(i(c(a(t(e)?)?)?)?)?|fin(d)?|mem(b(e(r)?)?)?|ind(e(x(o(f)?)?)?)?|ass(e(r(t)?)?)?|(readuntil|readbytes|fwriteall|imwrite|readstr|write|use))"
		},
		"mod1": {
			"name": "entity.name.type.uiua",
			"match": "([~/∧\\\\∵≡⫫⊞⊠⍥⊕⊜↶]|fol(d)?|eac(h)?|row(s)?|dis(t(r(i(b(u(t(e)?)?)?)?)?)?)?|tab(l(e)?)?|cro(s(s)?)?|repe(a(t)?)?|gro(u(p)?)?|par(t(i(t(i(o(n)?)?)?)?)?)?|inv(e(r(t)?)?)?|())"
		},
		"mod2": {
			"name": "keyword.control.uiua",
			"match": "([⍜⍛⍚⍣↰]|und(e(r)?)?|fil(l)?|lev(e(l)?)?|try|spa(w(n)?)?|())"
		}
	},
	"scopeName": "source.uiua"
}